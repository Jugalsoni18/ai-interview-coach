{
  "version": 2,
  "framework": null,
  "installCommand": "echo Skipping install for static site",
  "buildCommand": "echo Skipping build for static site",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|mp4|json|pdf))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}